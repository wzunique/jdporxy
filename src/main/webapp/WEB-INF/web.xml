    <!--</filter>-->
    <!--<filter-mapping>-->
        <!--<filter-name>CorsFilter</filter-name>-->
        <!--<url-pattern>/*</url-pattern>-->
    <!--</filter-mapping>-->



    <error-page>
        <error-code>404</error-code>
        <location>/error_404</location>
    </error-page>
</web-app>